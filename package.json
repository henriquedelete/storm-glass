{
  "name": "typescript-api",
  "version": "0.0.1",
  "description": "Typescript Node.Js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev src/server.ts",
    "start": "npm run build && node ./dist/src/server.js ",
    "lint": "eslint ./src ./test --ext .ts",
    "lint-ix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check src/**/.ts test/**/*.ts",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts' ",
    "test:unit": "jest",
    "test:functional": "jest --projects ./test --forceExit --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henriquedelete/naoseiumnomebom.git"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "API"
  ],
  "author": "Carlos Henrique seguido, copiado do @waldemar-neto",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/henriquedelete/naoseiumnomebom/issues"
  },
  "homepage": "https://github.com/henriquedelete/naoseiumnomebom#readme",
  "devDependencies": {
    "@types/config": "^3.3.3",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/module-alias": "^2.0.3",
    "@types/node": "^20.8.9",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "config": "^3.3.9",
    "express": "^4.18.2",
    "module-alias": "^2.2.3"
  }
}
